use_frameworks!

install! 'cocoapods', :deterministic_uuids => false

target 'Connect Example' do
  platform :ios, '8.0'
      
  pod 'Connect', :path => '../'

  target 'Connect Tests' do
    inherit! :search_paths

  end
end


target 'Connect Editor' do
    platform :osx, '10.10'
    
    pod 'Connect', :path => '../'
    pod 'TraceLog', '~> 1.0'
end