            <resource path="/{countryCode}">

                <method href="#readRegionCountry"/>
                <method href="#updateRegionCountry"/>
                <method href="#deleteRegionCountry"/>


                <resource path="/locations">


                    <resource path="{locationId">

                    </resource>
                </resource>

            </resource>
        </resource>

