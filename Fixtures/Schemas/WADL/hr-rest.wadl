    <resources base="http://localhost:8080/rest/">

        <doc title="resources" xml:lang="eng">
            This is the resources base for all contained resources.
        </doc>
        
        <resource path="/regions">
            
            <doc title="resource" xml:lang="eng">
                This is the specific resource for the following realtive path:
                
                /regions
            </doc>
            
